<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="@dimen/dp20"
        android:layout_gravity="center"
        >
        <ImageView
            android:layout_width="@dimen/dp24"
            android:src="@drawable/ic_arrow_left"
            android:id="@+id/image_back"
            android:layout_height="@dimen/dp24"
            android:layout_gravity="center"
            android:onClick="imageback"
            android:gravity="center"
            android:tint="@color/textColblack"></ImageView>

        <TextView
            android:id="@+id/text_search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Profile"
            android:textSize="@dimen/dp16"
            android:layout_marginLeft="@dimen/dp10"
            android:text="Profile"
            android:gravity="center"
            android:ellipsize="end"
            android:maxLines="2"
            android:fontFamily="@font/playfairdisplayregular"
            android:textColor="@color/textColblack"


            />

    </LinearLayout>



    <LinearLayout
        android:id="@+id/linearsignupscreen"
        android:layout_width="match_parent"

        android:visibility="visible"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="@dimen/dp120"
            android:layout_height="@dimen/dp120"
            android:layout_marginTop="@dimen/dp10"
            android:padding="@dimen/dp10"
            android:layout_gravity="center"
            android:src="@drawable/profilelogo"
            app:civ_border_width="@dimen/dp1"
            app:civ_border_overlay="true"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Shubham"
            android:layout_gravity="center"

            android:textSize="@dimen/dp16"
            android:paddingTop="@dimen/dp3"
            android:paddingBottom="@dimen/dp10"
            android:id="@+id/textnamesaved"
            android:enabled="false"
            android:fontFamily="@font/playfairdisplaybold"
            android:textColor="@color/textColblack"></TextView>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:background="@color/gray"
            android:layout_marginTop="@dimen/dp10"></View>

<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">



        <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/dp20">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Gender"
            android:textSize="@dimen/dp20"
            android:paddingBottom="@dimen/dp10"
            android:paddingTop="@dimen/dp10"
            android:textColor="@color/textColblack"
            android:fontFamily="@font/playfairdisplayregular"
            ></TextView>


        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:background="@color/gray"
            android:hint="">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textEmailAddress"
                android:id="@+id/ed_gendersaved"
                android:enabled="false"/>

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Date of Birth"
            android:textSize="@dimen/dp20"
            android:paddingBottom="@dimen/dp10"
            android:paddingTop="@dimen/dp20"
            android:textColor="@color/textColblack"
            android:fontFamily="@font/playfairdisplayregular"
            ></TextView>


        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:hint=""
            android:background="@color/gray"
            >

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:id="@+id/ed_dobsaved"
                android:layout_height="wrap_content"
                android:inputType="text"

                android:enabled="false"
                />

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Email"
            android:textSize="@dimen/dp20"
            android:paddingBottom="@dimen/dp10"
            android:paddingTop="@dimen/dp20"
            android:textColor="@color/textColblack"
            android:fontFamily="@font/playfairdisplayregular"
            ></TextView>


        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp10"
            android:hint=""
            android:background="@color/gray"
            >

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ed_emailsaved"
                android:inputType="text"

                android:enabled="false"/>

        </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:id="@+id/text_changepswd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp16"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginTop="@dimen/dp20"
                android:text="@string/change_pswd"
                android:gravity="center"
                android:ellipsize="end"
                android:maxLines="2"
                android:layout_gravity="bottom"
                android:fontFamily="@font/playfairdisplayregular"
                android:textColor="@color/textColblack"
                android:onClick="change"


                />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Continue"
            android:textSize="@dimen/dp20"
            android:layout_marginTop="@dimen/dp10"
            android:visibility="gone"
            android:id="@+id/text_continue"
            android:gravity="center"
            android:layout_gravity="center"
            android:textColor="@color/textColblack"
            android:fontFamily="@font/playfairdisplayregular"
            ></TextView>

    </LinearLayout>
    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:indeterminate="true"
        android:visibility="gone"
        android:layout_gravity="center"


        />

</FrameLayout>
    </LinearLayout>



</LinearLayout>